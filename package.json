{
  "name": "dgkeep",
  "version": "0.0.0",
  "description": "Configurable API for tracking game scores",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "chai": "^2.3.0",
    "pg": "^4.3.0",
    "sequelize": "^2.1.0",
    "underscore": "^1.8.3",
    "winston": "^1.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2",
    "pre-git": "^0.2.1",
    "sqlite3": "^3.0.6"
  },
  "scripts": {
    "start": "node index.js",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint **/*.js",
    "lint-index": "eslint index.js"
  },
  "pre-commit": [
    "npm run lint",
    "npm run lint-index"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eprouty/dgkeep.git"
  },
  "author": "Eric Prouty <e.prouty12@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/eprouty/dgkeep/issues"
  },
  "homepage": "https://github.com/eprouty/dgkeep"
}
