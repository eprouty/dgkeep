{
  "name": "dgkeep",
  "version": "0.3.2",
  "description": "Configurable API for tracking game scores",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "lodash": "^3.8.0",
    "pg": "^4.3.0",
    "pg-hstore": "^2.3.2",
    "pg-native": "^1.8.1",
    "q": "^1.3.0",
    "sequelize": "^3.1.0",
    "winston": "^1.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "pre-git": "^0.2.1"
  },
  "scripts": {
    "start": "node index.js",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
